<local:KeycapViewer x:Class="Aurora.Settings.DeviceLayoutViewer.Keycaps.Control_ImageKeycap"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Aurora.Settings.DeviceLayoutViewer.Keycaps"
             mc:Ignorable="d">
    <Grid>
        <Border x:Name="keyBorder" BorderBrush="Gray" BorderThickness="0" CornerRadius="5" Background="#FF191919" MouseDown="keyBorder_MouseDown" MouseMove="keyBorder_MouseMove" MouseLeave="keyBorder_MouseLeave" MouseEnter="keyBorder_MouseEnter"/>
    </Grid>
</local:KeycapViewer>
